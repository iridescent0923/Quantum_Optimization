    "import pennylane as qml\n",
    "from pennylane import numpy as np\n",
    "\n",
    "dev = qml.device(\"default.qubit\", wires = 2)\n",
    "\n",
    "@qml.qnode(dev)\n",
    "def circuit():\n",
    "    qml.Hadamard(0)\n",
    "    qml.CNOT([0, 1])\n",
    "    \n",
    "    # return qml.density_matrix(wires = range(2))\n",
    "    return qml.probs()\n",
    "\n",
    "print(circuit())"
   ]
  }
